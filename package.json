{
  "name": "mywebcra",
  "license": "MIT",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host --clearScreen false",
    "build": "tsc && BUILD_PATH=build vite build",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "test-vite": "vite-jest --no-cache",
    "test-jest": "jest",
    "tsc": "tsc",
    "tsc:watch": "tsc --watch",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "lint:fix": "eslint \"src/**/*.{js,jsx,ts,tsx,json}\" --fix",
    "lint:fix2": "eslint . --ext ts --ext tsx --ext js --quiet --fix",
    "circleci:validate_old": "circleci config validate",
    "clean": "rm -rf ./build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@linaria/atomic": "5.0.3",
    "@linaria/babel-preset": "5.0.4",
    "@linaria/core": "5.0.2",
    "@linaria/react": "5.0.3",
    "@linaria/webpack-loader": "5.0.4",
    "@reduxjs/toolkit": "^1.9.7",
    "dat.gui": "0.7.9",
    "happy-dom": "^10.8.0",
    "react": "18.2.0",
    "react-alice-carousel": "2.8.0",
    "react-dom": "18.2.0",
    "react-markdown": "9.0.1",
    "react-redux": "^8.1.3",
    "react-router-dom": "6.20.0",
    "redux-saga": "^1.2.3",
    "ts-jest": "^29.1.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@linaria/vite": "^5.0.4",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/dat.gui": "0.7.12",
    "@types/jest": "29.5.10",
    "@types/node": "20.10.0",
    "@types/react": "18.2.38",
    "@types/react-dom": "18.2.17",
    "@types/react-redux": "^7.1.31",
    "@types/react-router-dom": "5.3.3",
    "@types/redux-saga": "^0.10.5",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.54.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "linaria": "5.0.5",
    "react-test-renderer": "^18.2.0",
    "typescript": "5.3.2",
    "vite": "^5.0.2",
    "vite-jest": "^0.1.4",
    "vitest": "^0.34.6"
  }
}
