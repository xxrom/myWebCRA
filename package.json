{
  "name": "mywebcra",
  "license": "MIT",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host --clearScreen false",
    "build": "tsc && BUILD_PATH=build vite build",
    "test": "jest test --transformIgnorePatterns \"node_modules/(?!ui-core)/\" --env=jsdom",
    "tsc": "tsc",
    "tsc:watch": "tsc --watch",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "lint:fix": "eslint \"src/**/*.{js,jsx,ts,tsx,json}\" --fix",
    "lint:fix2": "eslint . --ext ts --ext tsx --ext js --quiet --fix",
    "circleci:validate_old": "circleci config validate",
    "clean": "rm -rf ./build"
  },
  "browserslist": {
    "production": [">0.2%", "not dead", "not op_mini all"],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@linaria/atomic": "4.2.8",
    "@linaria/babel-preset": "4.4.3",
    "@linaria/core": "4.2.8",
    "@linaria/react": "4.3.6",
    "@linaria/webpack-loader": "4.1.15",
    "@reduxjs/toolkit": "^1.9.7",
    "dat.gui": "0.7.9",
    "react": "18.2.0",
    "react-alice-carousel": "2.8.0",
    "react-dom": "18.2.0",
    "react-markdown": "8.0.6",
    "react-redux": "^8.1.3",
    "react-router-dom": "6.19.0",
    "redux-saga": "^1.2.3",
    "typescript": "5.3.2"
  },
  "devDependencies": {
    "@babel/preset-typescript": "7.23.3",
    "@linaria/vite": "^4.2.9",
    "@testing-library/jest-dom": "6.1.4",
    "@testing-library/react": "14.1.2",
    "@testing-library/user-event": "14.5.1",
    "@types/dat.gui": "0.7.12",
    "@types/jest": "29.5.10",
    "@types/node": "20.9.4",
    "@types/react": "18.2.38",
    "@types/react-dom": "18.2.17",
    "@types/react-router-dom": "5.3.3",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.3",
    "linaria": "4.1.15",
    "vite": "^5.0.2"
  }
}
