{
  "name": "mywebcra",
  "license": "MIT",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host --clearScreen false",
    "build": "tsc && BUILD_PATH=build vite build --outDir 'build'",
    "test": "vitest",
    "preview": "vite preview --outDir 'build'",
    "coverage": "vitest run --coverage",
    "tsc": "tsc",
    "tsc:watch": "tsc --watch",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "lint:fix": "eslint \"src/**/*.{js,jsx,ts,tsx,json}\" --fix",
    "lint:fix2": "eslint . --ext ts --ext tsx --ext js --quiet --fix",
    "circleci:validate_old": "circleci config validate",
    "clean": "rm -rf ./build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@linaria/atomic": "6.2.0",
    "@linaria/core": "^6.2.0",
    "@linaria/react": "^6.2.1",
    "@reduxjs/toolkit": "^2.2.5",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "@wyw-in-js/babel-preset": "^0.5.3",
    "@wyw-in-js/webpack-loader": "^0.5.3",
    "dat.gui": "0.7.9",
    "react": "18.3.1",
    "react-alice-carousel": "2.9.1",
    "react-dom": "18.3.1",
    "react-helmet": "^6.1.0",
    "react-markdown": "9.0.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "6.23.1",
    "redux-saga": "^1.3.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.6",
    "@babel/preset-react": "^7.24.6",
    "@babel/preset-typescript": "^7.24.6",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.5.2",
    "@types/dat.gui": "0.7.13",
    "@types/jest": "29.5.12",
    "@types/node": "20.13.0",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@types/react-helmet": "^6.1.11",
    "@types/react-redux": "^7.1.33",
    "@types/react-router-dom": "5.3.3",
    "@types/redux-saga": "^0.10.5",
    "@types/testing-library__jest-dom": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "@vitest/ui": "^1.6.0",
    "@wyw-in-js/vite": "^0.5.3",
    "eslint": "^9.4.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest-dom": "^5.4.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "4.6.2",
    "husky": "9.0.11",
    "jest-environment-jsdom": "^29.7.0",
    "linaria": "6.2.0",
    "prettier": "^3.2.5",
    "react-test-renderer": "^18.3.1",
    "typescript": "5.4.5",
    "vite": "^5.2.12",
    "vitest": "^1.6.0"
  }
}
